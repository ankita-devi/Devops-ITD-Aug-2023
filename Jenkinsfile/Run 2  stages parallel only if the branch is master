pipeline{
    agent any
    stages{
        stage('parallel'){
        parallel{
            when{
                branch 'main'
            }
            stage('A'){
                steps{
                    sh 'ls-lrt'
                
                }
            }
            stage('B'){
                steps{
                    sh 'sleep 5'
                }
            }
        }
         }
    }
}
